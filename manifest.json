{
    "manifest_version": 3,
    "name": "Medium to Freedium Redirect",
    "version": "1.0",
    "description": "Автоматически перенаправляет платные статьи через Freedium",
    "permissions": [
      "storage",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {}
  }